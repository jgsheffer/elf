{
  "name": "Where's My Elf?",
  "displayName": "Where's My Elf?",
  "expo": {
    "extra": {
      "eas": {
        "projectId": "7126aa48-5299-4473-b05d-30b32a5b3bb3"
      }    
    },
    "owner": "elfie",
    "name": "Where's My Elf?",
    "slug": "elfie-frontend",
    "scheme": ["whereismyelf", "whereismyelf-dev"],
    "version": "1.0.16",
    "orientation": "portrait",
    "icon": "./assets/icons/elfie_logo.png",
    "splash": {
      "image": "./assets/images/splash-screen-ios-mobile.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "jsEngine": "hermes",
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "icon": "./assets/icons/elfie_logo.png",
      "package": "com.educationtech.whereismyelf",
      "softwareKeyboardLayoutMode": "pan",
      "versionCode": 1,
      "splash": {
        "image": "./assets/images/splash-screen-ios-mobile.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      }
    },
    "ios": {
      "icon": "./assets/icons/elfie_logo.png",
      "supportsTablet": true,
      "bundleIdentifier": "com.educationtech.whereismyelf",
      "buildNumber": "1",
      "splash": {
        "image": "./assets/images/splash-screen-ios-mobile.png",
        "tabletImage": "./assets/images/splash-screen-ios-tablet.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/icons/elfie_logo.png",
      "splash": {
        "image": "./assets/images/splash-logo-web.png",
        "resizeMode": "contain",
        "backgroundColor": "#191015"
      },
      "bundler": "metro"
    },
    "plugins": [
      "expo-localization",
      "expo-asset",
      [
        "expo-build-properties",
        {
          "ios": {
            "newArchEnabled": false
          },
          "android": {
            "newArchEnabled": false
          }
        }
      ],
      [
        "react-native-iap",
        {
          "paymentProvider": "both"
        }
      ]
    ],
    "experiments": {
      "tsconfigPaths": true
    }
  },
  "ignite": {
    "version": "9.9.1"
  }
}
